{
  "manifest_version": 2,	
  "name": "Hide Most Visited Pages",
  "version": "0.1",
  "description": "Hides the \"Most Visited\" page link section in the New Tabs window.",
  "background": { "scripts": ["background.js"] },
  "permissions" : [
    "tabs", "https://*.google.com/webhp?sourceid=chrome-instant*", 
